{
  "name": "hitogui",
  "version": "24.3-alpha",
  "description": "HITO search interface",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "serve",
    "fmt": "prettier -w . --cache && xmllint --format scripts/preprocess.xsl -o scripts/preprocess.xsl",
    "lint": "oxlint js node",
    "download": "curl -s --no-clobber https://hitontology.eu/public/2024-03-hito_diagram.svg -o img/input.svg",
    "preprocess": "xslt3 -s:img/input.svg -xsl:scripts/preprocess.xsl -o:/tmp/test.svg",
    "downloadandpreprocess": "curl -s --no-clobber https://hitontology.eu/public/2024-03-hito_diagram.svg -o img/input.svg && xslt3 -s:img/input.svg -xsl:scripts/preprocess.xsl -o:img/test.svg",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitontology/gui.git"
  },
  "keywords": [
    "search"
  ],
  "author": "Konrad HÃ¶ffner",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/hitontology/gui/issues"
  },
  "homepage": "https://github.com/hitontology/gui#readme",
  "devDependencies": {
    "oxlint": "^0.3.1",
    "prettier": "^3.2.5",
    "serve": "^14.2.2",
    "vitest": "^1.5.0",
    "xslt-processor": "^2.3.1",
    "xslt3": "^2.6.0"
  }
}
